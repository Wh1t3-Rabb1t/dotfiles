{
    "title": "APP LAUNCHER.",
    "rules": [
        {
            "description": "App launcher bindings.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "left_command",
                                "left_option",
                                "left_control",
                                "left_shift"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'kitty'"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": [
                                "left_command",
                                "left_option",
                                "left_control",
                                "left_shift"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Brave Browser'"
                        }
                    ]
                }
            ]
        }
    ]
}
